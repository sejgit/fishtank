[Unit]
Description=fishtankup program service
After=multi-user.target

[Service]
User=pi
Type=simple
WorkingDirectory=/home/pi/fishtank
ExecStart=/usr/bin/python3 /home/pi/fishtank/fishtank_v4.py -l 65.5 -u 69.5 -n 'Basementfish' -s downfishtemp
Restart=always
RestartSec=30

[Install]
WantedBy=multi-user.target
